 <!DOCTYPE module SYSTEM "module.dtd">

 <module name="IC">
  <doc>
   <description>
     IC vision module, Kevin's graduation project.     
   </description>
   </doc>
  <settings>
    <dl_settings name="control">
        <dl_settings name="IC">
          <dl_setting var="IC_threshold_gt"  min="0" step="1"    max="255" module="IC/IC_slave" shortname="Tresh avg" param="IC_THRESHOLD"/>
          <dl_setting var="IC_threshold_gtstd"  min="0" step="1"    max="255" module="IC/IC_slave" shortname="Tresh std" param="IC_THRESHOLD_STD"/>
          <dl_setting var="IC_threshold_nn"  min="0" step="1"    max="255" module="IC/IC_slave" shortname="Tresh avg" param="IC_THRESHOLD"/>                    
          <dl_setting var="IC_flymode" min="0" step="1"    max="1"   module="IC/IC_slave" shortname="FlyMode" values="Stereo|Textons" handler="FlyModeButton"/>
          <dl_setting var="IC_learnmode" min="0" step="1"    max="3"   module="IC/IC_slave" shortname="LearnMode" values="none|textons|stereo|both" handler="LearnModeButton"/>
        </dl_settings>
     </dl_settings>
   </settings> 
<header>
    <file name="IC_slave.h"/>
  </header>
  <periodic fun="IC_periodic()" start="IC_start()" stop="IC_stop()" autorun="TRUE" freq="10"/>
  <makefile>
    <file name="IC_slave.c"/>
  </makefile>
 </module>