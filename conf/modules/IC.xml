 <!DOCTYPE module SYSTEM "module.dtd">

 <module name="IC">
  <doc>
   <description>
     IC vision module, Kevin's graduation project.     
   </description>
   </doc>
  <settings>
    <dl_settings name="control">
       <dl_settings name="hoer">
    <dl_setting var="vision_threshold" min="0" step="1" max="255"   module="IC/IC_slave" shortname="t" param="Vision_Threshold"/>
     </dl_settings>
     </dl_settings>
   </settings>
<header>
    <file name="IC_slave.h"/>
  </header>
  <periodic fun="IC_periodic()" start="IC_start()" stop="IC_stop()" autorun="TRUE" freq="10"/>
  <makefile>
    <file name="IC_slave.c"/>
  </makefile>
 </module>